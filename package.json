{
	"name": "obsidian-embed-mdc",
	"version": "3.2.0",
	"description": "This plugin auto-fetches page metadata to embed Notion-style link preview cards.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"copyBuild": "make-dir build && rimraf build/main.js build/manifest.json build/styles.css && cpy main.js manifest.json styles.css build/",
		"prepublish": "npm run build && npm run copyBuild",
		"publish": "npm run build && npm run version && npm run copyBuild"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"metadata",
		"embed",
		"preview",
		"link-preview",
		"notion",
		"cards"
	],
	"author": "SErAphLi",
	"contributors": [
		"Mikaleb"
	],
	"license": "MIT",
	"devDependencies": {
		"@types/he": "^1.2.3",
		"@types/lodash": "^4.17.14",
		"@types/mustache": "^4.1.2",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "^8.21.0",
		"@typescript-eslint/parser": "^8.21.0",
		"builtin-modules": "^3.2.0",
		"cpy-cli": "^5.0.0",
		"cz-conventional-changelog": "^3.3.0",
		"esbuild": "0.13.12",
		"make-dir-cli": "^3.0.0",
		"mustache": "^4.2.0",
		"obsidian": "^1.7.2",
		"rimraf": "^5.0.5",
		"tslib": "2.8.1",
		"typescript": "5.7.3"
	},
	"dependencies": {
		"he": "^1.2.0"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
